<p id="notice"><%= notice %></p>

<h1>Shift Actuals</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shift_actuals.each do |shift_actual| %>
      <tr>
        <td><%= shift_actual.date %></td>
        <td><%= shift_actual.name %></td>
        <td><%= link_to 'Show', shift_actual %></td>
        <td><%= link_to 'Edit', edit_shift_actual_path(shift_actual) %></td>
        <td><%= link_to 'Destroy', shift_actual, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shift Actual', new_shift_actual_path %>
